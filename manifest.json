{
  "manifest_version": 2,

  "name": "Tab Stats",
  "description": "A set of statistics about your tabs.",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "img/TabStats.png",
    "default_popup": "views/popup.html",
    "default_title" : "Tab Stats"
  },
  "background": {
    "scripts": ["/js/background.js"]
  }
}