{
  "manifest_version": 2,
  "name": "Tab Stats",
  "version": "2.0",
  "description": "Accumulates statistics of your tab usage",
  "icons": {
      "128": "img/tabstats-128.png",
      "16": "img/tabstats-16.png",
      "19": "img/tabstats-19.png",
      "32": "img/tabstats-32.png",
      "48": "img/tabstats-48.png"
  },
  "browser_action": {
      "default_icon": "img/TabStats.png",
      "default_popup": "views/popup.html",
      "default_title" : "Tab Stats"
  },
  "author": "John Akerman, Matt Begley",
  "background": {
      "scripts": ["/js/background.js"]
  },
  "homepage_url": "https://github.com/JohnAkerman/TabStats",
  "offline_enabled": true,
  "permissions": [
    "tabs"
  ]
}
